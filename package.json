{
  "name": "whatsappwablasrepack",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 2>&1 | tee dev.log",
    "build": "next build && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "start": "NODE_ENV=production bun .next/standalone/server.js 2>&1 | tee server.log",
    "start:dev": "next dev",
    "start:standalone": "bun .next/standalone/server.js",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "type-check": "tsc --noEmit",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "clean": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules",
    "build:profile": "next build --profile",
    "setup": "chmod +x setup.sh && ./setup.sh",
    "setup:quick": "bun run build && chmod +x restart.sh && ./restart.sh",
    "deploy": "bun run build && rm -rf .git && git add . && git commit -m 'Update from local'",
    "deploy:cpanel": "./cpanel-install.sh",
    "deploy:ubuntu": "./install.sh",
    "update": "./update.sh check",
    "update:install": "./update.sh install",
    "monitor": "cd mini-services/whatsapp-socket && bun run dev",
    "monitor:socket": "./mini-services/whatsapp-socket/index.ts",
    "backup": "tar -czf backup-$(date +%Y%m%d).tar.gz --exclude='.next' --exclude='node_modules' --exclude='.git' .",
    "optimize": "next build --profile && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "verify": "next build && ls -la .next/standalone",
    "check": "bun install && bun run build && ls -la .next/standalone",
    "test": "npm test",
    "health": "curl -f http://localhost:3000/api/health 2>/dev/null || echo 'Health check unavailable'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@mdxeditor/editor": "^3.39.1",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-accordion": "latest",
    "@radix-ui/react-alert-dialog": "latest",
