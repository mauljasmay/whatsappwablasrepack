    "@radix-ui/react-tooltip": "latest",
    "@reactuses/core": "latest",
    "@tanstack/react-query": "latest",
    "@tanstack/react-table": "latest",
    "class-variance-authority": "latest",
    "clsx": "^2.1.1",
    "cmdk": "latest",
    "critters": "^0.0.25",
    "date-fns": "latest",
    "embla-carousel-react": "latest",
    "framer-motion": "latest",
    "input-otp": "latest",
    "lucide-react": "latest",
    "next": "15.3.5",
    "next-auth": "latest",
    "next-intl": "latest",
